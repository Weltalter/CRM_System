<table>
    <caption style="background-color:white">Список клиентов</caption>

    <colgroup>
        <col span="1" style="background-color:lightsteelblue"/>
    </colgroup>

    <thead>
        <tr>
            <th>ID</th>
            <th>Фамилия</th>
            <th>Имя</th>
            <th>Отчество</th>
            <th>Дата рождения</th>
        </tr>
    </thead>

    <tbody>
        @{
            foreach(Client Client in Model.allClients) {
                <tr>
                    @Html.Partial("_SampleOutputClient", Client)
                </tr>
            }
        }
    </tbody>
</table>